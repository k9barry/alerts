{
    "name": "k9barry/alerts",
    "description": "Weather alerts API to SQLite database",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "monolog/monolog": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Alerts\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
